    android:padding="8dp">

    <TextView
        android:id="@+id/zag10"
        android:layout_width="10sp"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:textColor="#616161"
        android:textSize="13sp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/tvZag10"
        app:layout_constraintStart_toStartOf="@+id/zag06"
        app:layout_constraintTop_toTopOf="@+id/tvZag10"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btnGagaInfo2"
        android:layout_width="75dp"
        android:layout_height="40dp"
        android:layout_marginStart="60dp"
        android:padding="1dp"
        android:text="생산완료"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="@+id/tvZag08"
        app:layout_constraintTop_toBottomOf="@+id/tvZag08" />

    <Button
        android:id="@+id/btnGagaInfo"
        android:layout_width="75dp"
        android:layout_height="40dp"
