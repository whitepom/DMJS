        android:textSize="12.5sp"
        app:layout_constraintBottom_toBottomOf="@+id/tvZag08"
        app:layout_constraintStart_toEndOf="@+id/tvZag08"
        app:layout_constraintTop_toTopOf="@+id/tvZag08"
        app:layout_constraintVertical_bias="0.0"
